<html>
    <head><link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
</head>
    <body>
 
  <div class="container">
  <div class="row">
  <div class="col-md-12">
    <h1>this is the list of item meds</h1>
  </div>
  
  <div class="col-md-12">
         <a href="/index.html">Home</a>
         </div>
         <div class="col-md-12">
         <a href="dashboard">Dashboard</a>
         </div>
         <div class="col-md-12">
         <a href="#">Contact</a>
         </div>
        </div>
  <div class="row">
  <div class="col-md-12">
   <table class="table table-striped">
   <thead class="thead-dark">
   <tr>
    <th scope="col">Mid</th>
    <th scope="col">Medicine</th>
    <th scope="col">Count</th>
    <th scope="col">Brand</th>
    <th scope="col">Edit</th>
    <th scope="col">Delete</th>
    </tr>
   </thead>

{{#rows}}
      <tr>
      <td>{{mid}}</td>
      <td>{{name}}</td>
      <td>{{count}}</td>
      <td>{{brand}}</td>
      
       <td>
       <a class="btn btn-success" href="/meds/edit/{{mid}}">Edit</a>
       </td>

      <td>
      <form action="/meds/delete/{{mid}}" method="post">
      <button class="btn btn-danger" type="submit">Delete</button>
      </form>
      </td>
      </tr>
   {{/rows}}

   </table>
  </div>
  </div>
  </div>
</body>
</html>